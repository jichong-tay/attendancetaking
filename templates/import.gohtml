<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>Homepage</title>
  <link rel="stylesheet" href="/css/mystyle.css">
  <!-- Bootstrap CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">

</head>

<body>

  <div class="container-custom">
    <div class="container border border-primary">
      <div class="row justify-content-center">
        <div class="col-md-8 text-center">
          
          <div class="d-grid gap-2 col-8 mx-auto">
            <form enctype="multipart/form-data" action="/upload" method="POST" class="container-custom">
              <input class="input file-input" type="file" name="myFile" multiple />
              <button class="btn btn-secondary" type="submit">Upload</button>
            </form>
          </div>
          
          <div class="d-grid gap-2 col-4 mx-auto container">
           <a href="/restricted" class="btn btn-primary">Restricted</a>
           <a href="/" class="btn btn-primary">Home</a>
          </div>

        </div>
      </div>
    </div>
  </div>

</body>

</html>